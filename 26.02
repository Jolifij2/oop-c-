#include <iostream>
#include <cmath>

using namespace std;

class Circle {
private:
    double radius;
    const double PI = 3.14159265358979323846;
    
public:
    // Конструкторы
    Circle() : radius(1.0) {}
    Circle(double r) : radius(r > 0 ? r : 1.0) {}
    
    // Методы доступа
    double getRadius() const { return radius; }
    void setRadius(double r) { 
        if (r > 0) radius = r; 
    }
    
    // Длина окружности
    double getCircumference() const {
        return 2 * PI * radius;
    }
    
    // Площадь круга
    double getArea() const {
        return PI * radius * radius;
    }
    
    // Перегрузка оператора == (равенство радиусов)
    bool operator==(const Circle& other) const {
        return abs(radius - other.radius) < 0.000001; // Сравнение с точностью
    }
    
    // Перегрузка оператора > (сравнение длин окружностей)
    bool operator>(const Circle& other) const {
        return getCircumference() > other.getCircumference();
    }
    
    // Перегрузка оператора < (сравнение длин окружностей)
    bool operator<(const Circle& other) const {
        return getCircumference() < other.getCircumference();
    }
    
    // Перегрузка оператора += (увеличение радиуса)
    Circle& operator+=(double value) {
        if (radius + value > 0) {
            radius += value;
        }
        return *this;
    }
    
    // Перегрузка оператора -= (уменьшение радиуса)
    Circle& operator-=(double value) {
        if (radius - value > 0) {
            radius -= value;
        }
        return *this;
    }
    
    // Перегрузка оператора вывода
    friend ostream& operator<<(ostream& os, const Circle& c) {
        os << "Circle: radius = " << c.radius 
           << ", circumference = " << c.getCircumference()
           << ", area = " << c.getArea();
        return os;
    }
};

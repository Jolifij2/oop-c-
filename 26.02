#include <iostream>
#include <string>

using namespace std;

class Airplane {
private:
    string type;          // Тип самолета
    int maxPassengers;    // Максимальное количество пассажиров
    int currentPassengers;// Текущее количество пассажиров
    
public:
    // Конструкторы
    Airplane() : type("Unknown"), maxPassengers(100), currentPassengers(0) {}
    Airplane(string t, int maxP) : type(t), maxPassengers(maxP > 0 ? maxP : 100), currentPassengers(0) {}
    
    // Методы доступа
    string getType() const { return type; }
    int getMaxPassengers() const { return maxPassengers; }
    int getCurrentPassengers() const { return currentPassengers; }
    
    void setType(string t) { type = t; }
    void setCurrentPassengers(int p) { 
        if (p >= 0 && p <= maxPassengers) {
            currentPassengers = p;
        }
    }
    
    // Проверка возможности посадки пассажиров
    bool canBoard(int passengers) const {
        return currentPassengers + passengers <= maxPassengers;
    }
    
    // Проверка возможности высадки пассажиров
    bool canDisembark(int passengers) const {
        return currentPassengers - passengers >= 0;
    }
    
    // Перегрузка оператора == (равенство типов самолетов)
    bool operator==(const Airplane& other) const {
        return type == other.type;
    }
    
    // Перегрузка оператора != (неравенство типов самолетов)
    bool operator!=(const Airplane& other) const {
        return !(*this == other);
    }
    
    // Перегрузка оператора > (сравнение по максимальному количеству пассажиров)
    bool operator>(const Airplane& other) const {
        return maxPassengers > other.maxPassengers;
    }
    
    // Перегрузка оператора < (сравнение по максимальному количеству пассажиров)
    bool operator<(const Airplane& other) const {
        return maxPassengers < other.maxPassengers;
    }
    
    // Префиксный ++ (увеличение пассажиров на 1)
    Airplane& operator++() {
        if (currentPassengers < maxPassengers) {
            currentPassengers++;
        }
        return *this;
    }
    
    // Постфиксный ++ (увеличение пассажиров на 1)
    Airplane operator++(int) {
        Airplane temp = *this;
        ++(*this);
        return temp;
    }
    
    // Префиксный -- (уменьшение пассажиров на 1)
    Airplane& operator--() {
        if (currentPassengers > 0) {
            currentPassengers--;
        }
        return *this;
    }
    
    // Постфиксный -- (уменьшение пассажиров на 1)
    Airplane operator--(int) {
        Airplane temp = *this;
        --(*this);
        return temp;
    }
    
    // Перегрузка оператора вывода
    friend ostream& operator<<(ostream& os, const Airplane& a) {
        os << "Airplane: type = '" << a.type 
           << "', max passengers = " << a.maxPassengers
           << ", current passengers = " << a.currentPassengers;
        return os;
    }
};
